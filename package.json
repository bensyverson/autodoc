  "bin": {
    "autodoc": "bin/autodoc"
  },
  "scripts": {
    "test": "node bin/autodoc -t autodoc.js && mocha --compilers coffee:coffee-script"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dtao/autodoc.git"
  },
