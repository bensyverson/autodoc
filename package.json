  "bugs": {
    "url": "https://github.com/dtao/autodoc/issues"
  },
  "dependencies": {
    "benchmark": ">= 1.0.0",
    "commander": "*",
    "doctrine": "git://github.com/dtao/doctrine.git",
    "esprima": "*",
    "jasmine-node": "*",
    "lazy.js": ">= 0.1.2",
    "marked": "*",
    "mustache": ">= 0.7.2",
    "string-table": ">= 0.1.5"
  },
  "devDependencies": {
    "coffee-script": ">= 1.6.1",
    "mocha": ">= 1.13.0",
    "sinon": ">= 1.7.3",
    "should": ">= 2.0.2"
  }
