<!DOCTYPE html>
<html>

<head>
    <title>{{name}}</title>

    <link rel="stylesheet" type="text/css" href="http://agorbatchev.typepad.com/pub/sh/3_0_83/styles/shThemeDefault.css" />
    <link rel="stylesheet" type="text/css" href="docs.css" />
    <script src="http://pivotal.github.io/jasmine/lib/jasmine-1.3.1/jasmine.js"></script>
    <script src="http://lodash.com/_js/lodash.js"></script>
    <script src="http://benchmarkjs.com/_js/benchmark.js"></script>
    <script src="http://code.jquery.com/jquery-1.10.1.min.js"></script>
    <script src="http://code.highcharts.com/highcharts.js"></script>
    <script src="http://danieltao.com/HighTables/hightables.js"></script>
    <script src="http://agorbatchev.typepad.com/pub/sh/3_0_83/scripts/shCore.js" type="text/javascript"></script>
    <script src="http://agorbatchev.typepad.com/pub/sh/3_0_83/scripts/shBrushJScript.js" type="text/javascript"></script>
    <script type="text/javascript" src="utils.js"></script>
    <script type="text/javascript" src="docs.js"></script>

    {{#javascripts}}
    <script type="text/javascript" src="{{{.}}}"></script>
    {{/javascripts}}

    <script type="text/javascript" src="defaults.js"></script>

    <script type="text/javascript">
      var benchmarks = {};
    </script>
</head>

<body>
    <main>
        <header>
            <h1>{{name}}</h1>
        </header>

        <!-- Notification area for failing specs -->
        <div id="spec-failures"></div>

        <div class="layout">
            <div class="index">
                <nav>
                    <ul>
                        {{#namespaces}}
                        <li class="nav-header">
                            {{#constructorMethod}}
                            <a href="#{{identifier}}">{{name}}</a>
                            {{/constructorMethod}}
                            {{^constructorMethod}}
                            {{namespace}}
                            {{/constructorMethod}}
                        </li>
                        {{#members}}
                        {{^excludeFromDocs}}
                        <li><a href="#{{identifier}}">{{shortName}}</a></li>
                        {{/excludeFromDocs}}
                        {{/members}}
                        {{/namespaces}}
                    </ul>
                </nav>
            </div>

            <article>
                <section class="description">
                    {{{description}}}
                </section>

                {{#namespaces}}
                {{#allMembers}}
                {{^excludeFromDocs}}
                <section class="{{sectionType}}" id="{{identifier}}">
                    <h1>
                        {{shortName}}
                        <small>{{name}}</small>
                    </h1>

                    {{{description}}}

                    {{#hasSignature}}
                    <div class="signature">
                        <h3>Signature</h3>

                        <pre class="brush: js">{{{signature}}}</pre>

                        <table id="{{name}}-parameters">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Type(s)</th>
                                    <th>Description</th>
                                </tr>
                            </thead>

                            <tbody>
                                {{#params}}
                                <tr>
                                    <td>{{name}}</td>
                                    <td>{{type}}</td>
                                    <td>{{{description}}}</td>
                                </tr>
                                {{/params}}

                            </tbody>

                            {{^isConstructor}}
                            <tfoot>
                                {{#returns}}
                                <tr>
                                    <td>returns</td>
                                    <td>{{type}}</td>
                                    <td>{{{description}}}</td>
                                </tr>
                                {{/returns}}
                            </tfoot>
                            {{/isConstructor}}
                        </table>
                    </div>
                    {{/hasSignature}}

                    {{#hasExamples}}
                    <div class="examples">
                        <h3>Examples</h3>

                        {{#examples}}
                        <pre class="brush: js">{{{code}}}</pre>

                        <table id="{{name}}-examples">
                            <thead>
                                <tr>
                                    <th>Input</th>
                                    <th>Expected</th>
                                    <th>Result</th>
                                </tr>
                            </thead>

                            <tbody>
                                {{#list}}
                                <tr id="example-{{identifier}}-{{id}}">
                                    <td>{{{actual}}}</td>
                                    <td>{{{expected}}}</td>
                                    <td></td>
                                </tr>
                                {{/list}}
                            </tbody>
                        </table>
                        {{/examples}}
                    </div>
                    {{/hasExamples}}

                    {{#hasBenchmarks}}
                    <div class="perf">
                        <h3>Benchmarks</h3>

                        {{#benchmarks}}
                        <pre class="brush: js">{{{code}}}</pre>

                        <table id="{{identifier}}-perf-tests">
                            <!-- Gah, HighTables is stupid... doesn't account for <thead> :( -->
                            <tr>
                                <th>Implementation</th>
                                {{#cases}}
                                <th>{{label}}</th>
                                {{/cases}}
                            </tr>

                            {{#list}}
                            <tr id="perf-test-{{identifier}}-{{id}}">
                                <td>{{{name}}}</td>
                                {{#cases}}
                                <td data-case-id="{{caseId}}"></td>
                                {{/cases}}
                            </tr>
                            {{/list}}
                        </table>

                        <button>Run performance benchmarks</button>
                        {{/benchmarks}}
                    </div>
                    {{/hasBenchmarks}}

                    <script type="text/javascript">
                      {{> examples}}
                      {{> benchmarks}}
                    </script>
                </section>
                {{/excludeFromDocs}}
                {{/allMembers}}
                {{/namespaces}}
            </article>
        </div>
    </main>
</body>

</html>
