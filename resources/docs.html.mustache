<!DOCTYPE html>
<html>

<head>
    <title>{{name}}</title>

    <link rel="stylesheet" type="text/css" href="http://yandex.st/highlightjs/7.3/styles/default.min.css" />
    <link rel="stylesheet" type="text/css" href="docs.css" />
    <script src="http://pivotal.github.io/jasmine/lib/jasmine-1.3.1/jasmine.js"></script>
    <script src="http://lodash.com/_js/lodash.js"></script>
    <script src="http://benchmarkjs.com/_js/benchmark.js"></script>
    <script src="http://code.jquery.com/jquery-1.10.1.min.js"></script>
    <script src="http://code.highcharts.com/highcharts.js"></script>
    <script src="http://danieltao.com/HighTables/hightables.js"></script>
    <script src="http://yandex.st/highlightjs/7.3/highlight.min.js"></script>
    <script type="text/javascript" src="utils.js"></script>
    <script type="text/javascript" src="docs.js"></script>

    {{#javascripts}}
    <script type="text/javascript" src="{{{.}}}"></script>
    {{/javascripts}}

    <script type="text/javascript">
      var benchmarks = {};

      // A client library may have defined a custom assertEquality method, e.g.
      // in doc_helper.js; so we'll only use this default implementation if
      // necessary.
      if (typeof window.assertEquality === 'undefined') {
        window.assertEquality = function(actual, expected) {
          expect(expected).toEqual(actual);
        };
      }
    </script>
</head>

<body>
    <main>
        <header>
            <h1>{{name}}</h1>
        </header>

        <!-- Notification area for failing specs -->
        <div id="spec-failures"></div>

        <div class="layout">
            <div class="index">
                <nav>
                    <ul>
                        {{#namespaces}}
                        <li class="nav-header">
                            {{#constructorMethod}}
                            <a href="#{{identifier}}">{{name}}</a>
                            {{/constructorMethod}}
                            {{^constructorMethod}}
                            {{namespace}}
                            {{/constructorMethod}}
                        </li>
                        {{#members}}
                        <li><a href="#{{identifier}}">{{shortName}}</a></li>
                        {{/members}}
                        {{/namespaces}}
                    </ul>
                </nav>
            </div>

            <article>
                <section class="description">
                    {{{description}}}
                </section>

                {{#namespaces}}
                {{#allMembers}}
                <section class="{{sectionType}}" id="{{identifier}}">
                    <h1>
                        {{shortName}}
                        <small>{{name}}</small>
                    </h1>

                    {{{description}}}

                    {{#hasSignature}}
                    <div class="signature">
                        <h3>Signature</h3>

                        <pre><code>{{{signature}}}</code></pre>

                        <table id="{{name}}-parameters">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Type(s)</th>
                                    <th>Description</th>
                                </tr>
                            </thead>

                            <tbody>
                                {{#params}}
                                <tr>
                                    <td>{{name}}</td>
                                    <td>{{type}}</td>
                                    <td>{{{description}}}</td>
                                </tr>
                                {{/params}}

                            </tbody>

                            {{^isConstructor}}
                            <tfoot>
                                {{#returns}}
                                <tr>
                                    <td>returns</td>
                                    <td>{{type}}</td>
                                    <td>{{{description}}}</td>
                                </tr>
                                {{/returns}}
                            </tfoot>
                            {{/isConstructor}}
                        </table>
                    </div>
                    {{/hasSignature}}

                    {{#hasExamples}}
                    <div class="examples">
                        <h3>Examples</h3>

                        {{#examples}}
                        <pre><code>{{{code}}}</code></pre>

                        <table id="{{name}}-examples">
                            <thead>
                                <tr>
                                    <th>Input</th>
                                    <th>Expected</th>
                                    <th>Result</th>
                                </tr>
                            </thead>

                            <tbody>
                                {{#examples}}
                                <tr id="example-{{identifier}}-{{id}}">
                                    <td>{{{input}}}</td>
                                    <td>{{{output}}}</td>
                                    <td></td>
                                </tr>
                                {{/examples}}
                            </tbody>
                        </table>
                        {{/examples}}
                    </div>
                    {{/hasExamples}}

                    {{#hasBenchmarks}}
                    <div class="perf">
                        <h3>Benchmarks</h3>

                        {{#benchmarks}}
                        <pre><code>{{{code}}}</code></pre>

                        <table id="{{identifier}}-perf-tests">
                            <!-- Gah, HighTables is stupid... doesn't account for <thead> :( -->
                            <tr>
                                <th>Implementation</th>
                                {{#cases}}
                                <th>{{label}}</th>
                                {{/cases}}
                            </tr>

                            {{#benchmarks}}
                            <tr id="perf-test-{{identifier}}-{{id}}">
                                <td>{{{name}}}</td>
                                {{#cases}}
                                <td data-case-id="{{caseId}}"></td>
                                {{/cases}}
                            </tr>
                            {{/benchmarks}}
                        </table>

                        <button>Run performance benchmarks</button>
                        {{/benchmarks}}
                    </div>
                    {{/hasBenchmarks}}

                    <script type="text/javascript">
                      describe('{{{name}}}', function() {
                        {{#examples}}

                        {{{setup}}}

                        {{#examples}}
                        (function() {
                          var spec = it('returns {{output}} for {{input}}', function() {
                            var result = {{{input}}};
                            {{#hasCustomHandler}}
                            var handler = exampleHandlers[{{handlerIndex}}],
                                match   = {{{outputPattern}}}.match(handler.pattern);

                            handler.test(match, result);
                            {{/hasCustomHandler}}
                            {{^hasCustomHandler}}
                            assertEquality(result, {{{output}}});
                            {{/hasCustomHandler}}
                          });

                          spec.exampleId = '{{identifier}}-{{id}}';
                          spec.input = '{{input}}';
                          spec.output = '{{output}}';
                        }());

                        {{/examples}}
                        {{/examples}}
                      });

                      (function() {
                        {{#benchmarks}}

                        {{{setup}}}

                        benchmarks['{{identifier}}'] = [];

                        {{#benchmarks}}
                        {{#cases}}
                        (function() {
                          var benchmark = new Benchmark('{{name}}', {
                            id: '{{identifier}}-{{id}}-{{caseId}}',
                            fn: function() {
                              {{{impl}}};
                            }
                          });

                          benchmark.benchmarkId = '{{identifier}}-{{id}}';
                          benchmark.caseId = {{caseId}};

                          benchmarks['{{identifier}}'].push(benchmark);
                        }());

                        {{/cases}}
                        {{/benchmarks}}
                        {{/benchmarks}}
                      }());
                    </script>
                </section>
                {{/allMembers}}
                {{/namespaces}}
            </article>
        </div>
    </main>
</body>

</html>
